ost('/admin/queryAllTeacher').then((m) => {\r\n  \t\t\tthis.tTeacherList = m.content.tTeacherList;\r\n  \t\t})\r\n  \t},\r\n  \tqueryAllAuthority() {\r\n  \t\tthis.$post('/admin/queryAllAuthority').then((m) => {\r\n  \t\t\tthis.roleList = m.content.roleList;\r\n  \t\t\tthis.roleList.forEach((item, index) => {\r\n  \t\t\t\tif(item.ifCheck === 1) {\r\n  \t\t\t\t\tthis.arr[index].checked = true;\r\n  \t\t\t\t}\r\n  \t\t\t})\r\n  \t\t})\r\n  \t},\r\n  \tqueryAccountAuthorityList() {\r\n  \t\tthis.$post('/admin/queryAccountAuthorityList',{\"page\": this.currentPage - 1,\r\n\t\t\t\t\t\"size\": this.pageSize}).then((m) => {\r\n\t\t\t\tthis.userRoleList = m.content.page;\r\n  \t\t\tthis.tableData = m.content.page.content;\r\n  \t\t})\r\n  \t},\r\n  \t// 创建权限管理账号\r\n  \tsaveAccountAuthority() {\r\n  \t\tif(!!!this.userName) {\r\n  \t\t\treturn this.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请选择人员名称！'\r\n\t\t\t\t});\r\n  \t\t}\r\n  \t\tif(!!!this.roleName) {\r\n  \t\t\treturn this.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请选择权限！'\r\n\t\t\t\t});\r\n  \t\t}\r\n  \t\tif(!!!this.townName && this.roleName<'110005') {\r\n  \t\t\treturn this.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请选择归属区县！'\r\n\t\t\t\t});\r\n  \t\t}\r\n\t\t\tthis.$post('/admin/saveAccountAuthority', {'name': this.userName, 'roleName': this.roleName, 'townName': this.townName}).then((m) => {\r\n\t\t\t\tif(m.resultCode == 0) {\r\n    \t\t\tthis.$message({\r\n\t    \t\t\tshowClose: true,\r\n\t\t\t\t\t\ttype: 'success',\r\n\t\t\t\t\t\tmessage: '操作成功！'\r\n\t\t\t\t\t});\r\n\t\t\t\t\tthis.userName = '';\r\n\t\t\t\t\tthis.roleName = '';\r\n\t\t\t\t\tthis.townName = '';\r\n\t\t\t\t\tthis.queryAccountAuthorityList();\r\n    \t\t}else if(m.resultCode == -1) {\r\n    \t\t\tthis.$message({\r\n\t    \t\t\tshowClose: true,\r\n\t\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\t\tmessage: m.resu