D,GAAI,QAGJ4D,MAAO,eACP5D,GAAI,WAMC3B,IAET2B,GAAI,EACJzB,KAAM,MACNgG,OAAQ,GACRC,QAAS,KAGTxE,GAAI,EACJzB,KAAM,MACNgG,OAAQ,GACRC,QAAS,KAGTxE,GAAI,EACJzB,KAAM,MACNgG,OAAQ,GACRC,QAAS,KAGTxE,GAAI,EACJzB,KAAM,MACNgG,OAAQ,GACRC,QAAS","file":"static/js/6.cb296641b7dc018408b2.js","sourcesContent":["//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\r\nimport { addTable, county } from './json'\r\nimport { dataFormat } from '../../frame_template/js/utils'\r\nconst COLORS = ['#fee9e9', '#e1f5fe', '#e8f6ed'];\r\nexport default {\r\n  data() {\r\n    return {\r\n      value: '',\r\n      projectStage: '', //项目阶段\r\n      options: [],\r\n      tableData: [],\r\n      editId: sessionStorage.getItem(\"EditCityProjectID\"),\r\n      county: county,\r\n      forms: {\r\n      \tname: '', //项目名称\r\n      \tstartTime: '', //开工年份\r\n      \tendTime: '', //竣工年份\r\n      \tcontacts: '', //总负责人\r\n      \tinvest: '', //规划总投资\r\n      \tprocedureType: '', //程序流程id\r\n      \tprojectData: '', //项目阶段、进度数据\r\n      \ttownData: '' //区县范围数据\r\n      },\r\n      townList: [], //区县范围列表\r\n      checkTownList: [], //选中的区县范围列表\r\n      projectTownList: [],\r\n      procedureList: []\r\n    }\r\n  },\r\n  \r\n  // 页面初始化\r\n\tcreated: function() {\r\n\t\t\t\r\n\t\t\tthis.findProcedureList();\r\n\t},\r\n  \r\n  mounted() {\r\n    this.$nextTick(() => {\r\n    \tif(!!this.editId) {\r\n    \t\tthis.queryProjectById();\r\n    \t\tthis.queryTownInfo(this.editId);\r\n    \t}\r\n      this.getSpanArr(addTable);\r\n      this.tableData = addTable;\r\n    })\r\n  },\r\n\r\n  methods: {\r\n  \tqueryProjectById() {\r\n  \t\tthis.$post('/admin/queryProjectById', {'id': this.editId}).then((m) => {\r\n  \t\t\tlet obj = m.content.tProject;\n  \t\t\tlet tProjectStageList = m.content.tProjectStageList;\n  \t\t\t//组装前台数据\r\n  \t\t\tlet newArrays = [];\r\n  \t\t\ttProjectStageList.forEach((tProjectStage, index) => {\r\n  \t\t\t\tlet tableObj = {};\r\n  \t\t\t\ttableObj.id = tProjectStage.id;\r\n  \t\t\t\ttableObj.type = index++;\r\n  \t\t\t\ttableObj.name = tProjectStage.name;\r\n  \t\t\t\ttableObj.isDao = tProjectStage.isDaopai;\r\n  \t\t\t\ttableObj.plan = tProjectStage.plan;\r\n  \t\t\t\tnewArrays.push(tableObj);\r\n  \t\t\t})\r\n\t\t\t\tthis.getSpanArr(newArrays);\r\n  \t\t\tthis.forms = obj;\r\n  \t\t\tthis.tableData = newArrays;\r\n  \t\t})\r\n  \t},\r\n    //合并单元格统计\r\n    getSpanArr(data) {\r\n      this._spanArr = [];\r\n      for (var i = 0; i < data.length; i++) {\r\n        if (i === 0) {\r\n          this._spanArr.push(1);\r\n          this._pos = 0;\r\n        } else {\r\n          // 判断当前元素与上一个元素是否相同\r\n          if (data[i].id === data[i - 1].id) {\r\n            this._spanArr[this._pos] += 1;\r\n            this._spanArr.push(0);\r\n          } else {\r\n            this._spanArr.push(1);\r\n            this._pos = i;\r\n          }\r\n        }\r\n      }\r\n    },\r\n\r\n    //合并单元格\r\n    arraySpanMethod({ row, column, rowIndex, columnIndex }) {\r\n      if ( columnIndex === 0 ) {\r\n        const _row = this._spanArr[rowIndex];\r\n        const _col = _row > 0 ? 1 : 0;\r\n        return {\r\n          rowspan: _row,\r\n          colspan: _col\r\n        };\r\n      }\r\n\r\n      if( columnIndex === 2 && (row.isAdd || row.isEdit) ) {\r\n        return {\r\n          rowspan: 1,\r\n          colspan: 0\r\n        };\r\n      }\r\n\r\n      if( columnIndex === 3 && (row.isAdd || row.isEdit) ) {\r\n        return {\r\n          rowspan: 1,\r\n          colspan: 2\r\n        };\r\n      }\r\n    },\r\n    \r\n    //设置列样式\r\n    setCellStyle({row, column, rowIndex, columnIndex}) {\r\n      if( columnIndex === 0 ) {\r\n        this._index === undefined ? this._index = 0 : this._index++;\r\n        return {\r\n          backgroundColor: COLORS[this._index % 3],\r\n          fontSize: '18px',\r\n        }\r\n      }else {\r\n        return {\r\n          color: '#606266'\r\n        }\r\n      }\r\n    },\r\n\r\n    //添加进度\r\n    addPlan(row, index) {\r\n      if( row.plan === '' ) {\r\n        return this.$message({\r\n          showClose: true,\r\n          message: '请输入进度名称！',\r\n          type: 'error'\r\n        });\r\n      }\r\n\r\n      if( !!row.isEdit ) return row.isEdit = false;\r\n\r\n      let obj = JSON.parse(JSON.stringify(row))\r\n      obj.isAdd = false;\r\n      this.tableData.splice(index, 0, obj)\r\n      this.getSpanArr(this.tableData);\r\n    },\r\n\r\n    // 删除进度\r\n    delPlan(row, index) {\r\n      this.tableData.splice(index, 1);\r\n      this.getSpanArr(this.tableData);\r\n    },\r\n\r\n    planEdit(row) {\r\n      if(!!row.isEdit) return;\r\n      row.isEdit = true;\r\n    },\r\n\r\n    //进度排序\r\n    planSort(row, index, sort) {\r\n      if( sort === 'up' ) {\r\n        let f = index - 1 >= 0\r\n              && this.tableData[index].id !== this.tableData[index-1].id;\r\n        if( f || index === 0 ) return;\r\n\r\n        this.tableData.splice(index, 1);\r\n        this.tableData.splice(index-1, 0, row);\r\n\r\n      }else {\r\n        let f = this.tableData.length > index + 1 \r\n              && !!this.tableData[index+1].isAdd;\r\n        if( f ) return;\r\n\r\n        this.tableData.splice(index, 1);\r\n        this.tableData.splice(index+1, 0, row);\r\n      }\r\n    },\r\n\r\n    closeEdit(row) {\r\n      if( !!row.isEdit ) {\r\n        row.isEdit = false;\r\n      }else {\r\n        row.plan = '';\r\n        row.selectValue = '';\r\n      }\r\n    },\r\n\r\n    handlerCheckbox(type) {\r\n      //全选\r\n      if( type === 1 ) {\r\n        this.townList.forEach(item => {\r\n        \tif( typeof item.isChecked === 'undefined' ) {\r\n        \t\tthis.$set(item, 'isChecked', true);\r\n        \t}else{\r\n        \t\titem.isChecked = true;\r\n        \t}\r\n        })\r\n      }\r\n\r\n      //取消\r\n      if( type === 2 ) {\r\n        this.townList.forEach(item => {\r\n        \tif( typeof item.isChecked === 'undefined' ) {\r\n        \t\tthis.$set(item, 'isChecked', false);\r\n        \t}else{\r\n        \t\titem.isChecked = false;\r\n        \t}\r\n        })\r\n      }\r\n\r\n      //反选\r\n      if( type === 3 ) {\r\n        this.townList.forEach(item => {\r\n        \tif( typeof item.isChecked === 'undefined' ) {\r\n        \t\tthis.$set(item, 'isChecked', true);\r\n        \t}else{\r\n        \t\titem.isChecked = !item.isChecked;\r\n        \t}\r\n        })\r\n      }\r\n    },\r\n    changeProcedure(data) {\r\n    \tthis.$post('/admin/queryAllProcedureById', {'id': data}).then((m) => {\r\n    \t\tlet procedureList = m.content.procedureList;\r\n    \t\tprocedureList.forEach((item, index) => {\r\n    \t\t\tlet obj = {};\r\n  \t\t\t\tobj.label = item.name;\r\n  \t\t\t\tobj.id = item.id;\r\n  \t\t\t\tthis.procedureList.push(obj);\r\n    \t\t})\r\n    \t\tthis.tableData.forEach((data, index) => {\r\n    \t\t\tdata.flowPath = this.procedureList;\r\n    \t\t})\r\n    \t})\r\n    },\r\n    //保存项目阶段\r\n    saveProjectStage() {\r\n    \tif(!!!this.projectStage) {\r\n    \t\tthis.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请输入阶段名称！'\r\n\t\t\t\t});\r\n\t\t\t\treturn false;\r\n    \t}else{\r\n    \t\tlet obj = {};\r\n    \t\tobj.id = 2222;\r\n    \t\tobj.type = 4;\r\n    \t\tobj.name = this.projectStage;\r\n    \t\tobj.plan = \"\";\r\n    \t\tobj.isAdd = true;\r\n    \t\tobj.flowPath = [];\r\n    \t\tthis.tableData.splice(this.tableData.length, 0, obj);\r\n    \t\tthis.getSpanArr(this.tableData);\r\n    \t\tthis.projectStage = \"\";\r\n    \t}\r\n    },\r\n    //\r\n    queryTownInfo(id) {\r\n    \tthis.$post('/web/queryTownInfo', {\"projectId\": id}).then((m) => {\r\n    \t\tthis.townList = m.content.tDictionaryList;\n    \t\tthis.projectTownList = m.content.tProjectTownList;\r\n    \t\tthis.townList.forEach((item, index) => {\r\n    \t\t\tthis.$set(item, 'danwei', '');\r\n    \t\t\tthis.$set(item, 'lingdao', '');\r\n    \t\t\tthis.$set(item, 'isChecked', false);\r\n    \t\t\tthis.projectTownList.forEach((town) => {\r\n    \t\t\t\tif(town.townId == item.code) {\r\n    \t\t\t\t\titem.isChecked = true;\r\n    \t\t\t\t\tif(!!town.accountabilityUnit) {\r\n\t    \t\t\t\t\titem.danwei = town.accountabilityUnit;\r\n\t    \t\t\t\t}\r\n\t    \t\t\t\tif(!!town.contacts) {\r\n\t    \t\t\t\t\titem.lingdao = town.contacts;\r\n\t    \t\t\t\t}\r\n    \t\t\t\t}\r\n    \t\t\t})\r\n    \t\t\t\r\n    \t\t})\r\n\t\t\t})\r\n    },\r\n    //查询程序流程类型列表\r\n    findProcedureList() {\r\n    \tthis.$post('/web/findProcedureList').then((m) => {\r\n    \t\tthis.options = m.content.tProcedureTypeList;\r\n    \t})\r\n    },\r\n    //验证开工时间\r\n    validStartTime(data) {\r\n    \tif(!!this.forms.endTime && !!data) {\r\n    \t\tif(this.forms.endTime < data.getTime()) {\r\n    \t\t\tthis.$message({\r\n\t    \t\t\tshowClose: true,\r\n\t\t\t\t\t\ttype: 'warning',\r\n\t\t\t\t\t\tmessage: '竣工时间应大于开工时间！'\r\n\t\t\t\t\t});\r\n\t\t\t\t\tthis.forms.endTime = '';\r\n\t\t\t\t\treturn false;\r\n    \t\t}\r\n    \t}\r\n    },\r\n    //验证竣工时间\r\n    validEndTime(data) {\r\n    \tif(!!this.forms.startTime && !!data) {\r\n    \t\tif(this.forms.startTime > data.getTime()) {\r\n    \t\t\tthis.$message({\r\n\t    \t\t\tshowClose: true,\r\n\t\t\t\t\t\ttype: 'warning',\r\n\t\t\t\t\t\tmessage: '开工时间应小于竣工时间！'\r\n\t\t\t\t\t});\r\n\t\t\t\t\tthis.forms.startTime = '';\r\n\t\t\t\t\treturn false;\r\n    \t\t}\r\n    \t}\r\n    },\r\n    //保存\r\n    finalSave() {\r\n    \tif(!!!this.forms.name) {\r\n    \t\tthis.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请输入项目名称！'\r\n\t\t\t\t});\r\n\t\t\t\treturn false;\r\n    \t}\r\n    \tif(!!!this.forms.startTime) {\r\n    \t\tthis.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请选择开工日期！'\r\n\t\t\t\t});\r\n\t\t\t\treturn false;\r\n    \t}\r\n    \tif(!!!this.forms.endTime) {\r\n    \t\tthis.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请选择竣工日期！'\r\n\t\t\t\t});\r\n\t\t\t\treturn false;\r\n    \t}\r\n    \tif(!!!this.forms.contacts) {\r\n    \t\tthis.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请输入总负责人！'\r\n\t\t\t\t});\r\n\t\t\t\treturn false;\r\n    \t}\r\n    \tif(!!!this.forms.invest) {\r\n    \t\tthis.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请输入规划总投资！'\r\n\t\t\t\t});\r\n\t\t\t\treturn false;\r\n    \t}\r\n    \tif(!!!this.forms.procedureType) {\r\n    \t\tthis.$message({\r\n    \t\t\tshowClose: true,\r\n\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\tmessage: '请选择程序流程！'\r\n\t\t\t\t});\r\n\t\t\t\treturn false;\r\n    \t}\r\n    \t//阶段 进度 操作流程\r\n    \tthis.forms.projectData = this.tableData;\r\n    \tthis.forms.startTime = dataFormat(new Date(this.forms.startTime), 'YYYY-MM-DD HH:mm:ss');\r\n    \tthis.forms.endTime = dataFormat(new Date(this.forms.endTime), 'YYYY-MM-DD HH:mm:ss');\r\n    \tthis.townList.forEach((item, index) => {\r\n    \t\tif(item.isChecked) {\r\n    \t\t\tif(!!!item.danwei) {\r\n    \t\t\t\tthis.$message({\r\n\t\t    \t\t\tshowClose: true,\r\n\t\t\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\t\t\tmessage: '请输入责任单位！'\r\n\t\t\t\t\